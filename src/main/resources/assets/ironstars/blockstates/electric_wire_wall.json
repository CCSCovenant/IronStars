    {
      "when": {"east": "ul"},
      "apply": {"model": "ironstars:block/wire/wall/corner","y": 90}
    },
    {
      "when": {"east": "ur"},
      "apply": {"model": "ironstars:block/wire/wall/corner","z": 90,"y": 90}
    },
    {
      "when": {"east": "dr"},
