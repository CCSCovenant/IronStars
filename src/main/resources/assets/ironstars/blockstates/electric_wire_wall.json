{
  "multipart": [
    {
      "when": {"north": "ud"},
      "apply": {"model": "ironstars:block/wire/wall/line"}
    },
    {
      "when": {"north": "lr"},
      "apply": {"model": "ironstars:block/wire/wall/line","z": 90}
    },
    {
      "when": {"north": "ul"},
      "apply": {"model": "ironstars:block/wire/wall/corner"}
    },
    {
      "when": {"north": "ur"},
      "apply": {"model": "ironstars:block/wire/wall/corner","z": 90}
    },
    {
      "when": {"north": "dr"},
      "apply": {"model": "ironstars:block/wire/wall/corner","z": 180}
    },
    {
      "when": {"north": "dl"},
      "apply": {"model": "ironstars:block/wire/wall/corner","z": 270}
    },


    {
      "when": {"east": "ud"},
      "apply": {"model": "ironstars:block/wire/wall/line","y": 90}
    },
    {
      "when": {"east": "lr"},
      "apply": {"model": "ironstars:block/wire/wall/line","z": 90,"y": 90}
