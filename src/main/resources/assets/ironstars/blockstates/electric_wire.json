    {
      "when": {"west": "ul"},
      "apply": {"model": "ironstars:block/wire/wall_corner","y": 270}
    },
    {
      "when": {"west": "ur"},
      "apply": {"model": "ironstars:block/wire/wall_corner","z": 90,"y": 270}
