{
  "multipart": [
    {
      "when": {"down": "ns"},
      "apply": {"model": "ironstars:block/wire/straight"}
    },
    {
      "when": {"down": "we"},
      "apply": {"model": "ironstars:block/wire/straight","y": 90}
    },
    {
      "when": {"down": "nw"},
      "apply": {"model": "ironstars:block/wire/corner"}
    },
    {
      "when": {"down": "ne"},
      "apply": {"model": "ironstars:block/wire/corner","y": 90}
    },
    {
      "when": {"down": "se"},
      "apply": {"model": "ironstars:block/wire/corner","y": 180}
    },
    {
      "when": {"down": "sw"},
      "apply": {"model": "ironstars:block/wire/corner","y": 270}
    },


    {
      "when": {"up": "ns"},
      "apply": {"model": "ironstars:block/wire/straight","z": 180}
    },
    {
      "when": {"up": "we"},
      "apply": {"model": "ironstars:block/wire/straight","y": 90,"z": 180}
    },
    {
      "when": {"up": "ne"},
      "apply": {"model": "ironstars:block/wire/corner","z": 180}
